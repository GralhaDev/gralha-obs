{
    "name": "yurivecchi/gralhaobs",
    "description": "OBS Extension for Twitch.tv",
    "type": "project",
    "license": "FREE",
    "authors": [
        {
            "name": "Yuri Vecchi",
            "email": "yurivecchi@gralha.dev"
        }
    ],
    "require": {
        "php": "7.4.*",
        "guzzlehttp/guzzle": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "GralhaObs\\": "src\\",
            "Config\\": "config\\"
        }
    },
    "config": {
        "process-timeout": 0
    },
    "scripts": {
        "followers": "php -S 0.0.0.0:7001 followers.php",
        "subscribers": "php -S 0.0.0.0:7002 subscribers.php"
    }
}
